			<activeWhen>
				<and>
					<reference definitionId="org.eclipse.efbt.cocason.dsl.cocason.CoCaSon.XtextEditor.opened" />
					<iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
			</activeWhen>
		</handler>
